{
  "name": "dateprettify",
  "version": "1.2.0",
  "description": "Simple Date Formatter utility using moment",
  "main": "dist/dateprettify.umd.js",
  "scripts": {
		"prepublish": "rollup -c",
		"rollup:tests": "rollup -i tests/tests.js -f cjs -o tests/tests.cjs.js -e tape,moment && rollup -c",
		"pretest": "npm run rollup:tests",
    "test": "node tests/tests.cjs.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/dateprettify.git"
  },
  "keywords": [
    "dates",
    "prettify",
    "moment",
    "formatting"
  ],
  "author": "Dustin Hershman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhershman1/dateprettify/issues"
  },
  "homepage": "https://github.com/dhershman1/dateprettify#readme",
  "dependencies": {
    "moment": "^2.13.0"
  },
  "devDependencies": {
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
